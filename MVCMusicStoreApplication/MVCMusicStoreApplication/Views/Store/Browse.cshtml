@model IEnumerable<MVCMusicStoreApplication.Models.Genre>

@{
    ViewBag.Title = "Browse";
}



 <h2> Browse by Genre</h2>                
        <div>
            @foreach (var item in Model)
            {
                <ul>
                    <li>
                        @Html.ActionLink(item.Name, "Index", new { id = @item.GenreId })
                    </li>
                </ul>
            }
        </div>
